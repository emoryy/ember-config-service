{
  "name": "ember-config-service",
  "version": "1.0.0",
  "description": "Access config values from config/environment.js with a simple service",
  "keywords": [
    "ember-addon",
    "config",
    "configuration",
    "environment",
    "service"
  ],
  "repository": "https://github.com/gmurphey/ember-config-service",
  "license": "MIT",
  "author": "Garrett Murphey <gmurphey@gmurphey.com>",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build",
    "lint:hbs": "ember-template-lint .",
    "lint:js": "eslint .",
    "start": "ember serve",
    "test": "ember test",
    "test:all": "ember try:each",
    "postpublish": "git push origin $(git branch | grep '*' | sed 's/* //'); git push origin --tags"
  },
  "dependencies": {
    "ember-cli-babel": "^7.26.11",
    "ember-getowner-polyfill": "3.0.2"
  },
  "devDependencies": {
    "@ember/optional-features": "^2.0.0",
    "broccoli-asset-rev": "^3.0.0",
    "ember-cli": "~4.2.0",
    "ember-cli-dependency-checker": "^3.2.0",
    "ember-cli-eslint": "^5.1.0",
    "ember-cli-htmlbars": "^6.0.1",
    "ember-cli-htmlbars-inline-precompile": "^3.0.1",
    "ember-cli-inject-live-reload": "^2.1.0",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-template-lint": "^1.0.0",
    "ember-cli-uglify": "^3.0.0",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-export-application-global": "^2.0.1",
    "ember-load-initializers": "^2.1.2",
    "ember-maybe-import-regenerator": "^1.0.0",
    "ember-qunit": "^5.1.5",
    "ember-resolver": "^8.0.3",
    "ember-source": "~4.2.0",
    "ember-source-channel-url": "^3.0.0",
    "ember-try": "^2.0.0",
    "eslint-plugin-ember": "^10.5.9",
    "eslint-plugin-node": "^11.1.0",
    "loader.js": "^4.7.0",
    "qunit-dom": "^2.0.0"
  },
  "engines": {
    "node": "8.* || >= 10.*"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  }
}
